{
  "manifest_version": 2,
  "name": "Good Search",
  "description": "Good Search",
  "version": "1.0",
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html",
    "default_title": "Good Search"
  },
  "background": {
    "scripts": ["jquery.js", "background.js"]
  },
  "commands": {
    "google": {
      "suggested_key": {
        "windows": "Ctrl+G",
        "mac": "Command+G"
      },
      "global": true,
      "description": "google"
    }
  },
  "permissions": [
    "tabs",
    "activeTab",
    "https://ajax.googleapis.com/",
    "webRequest",
    "webRequestBlocking",
    "https://*/*",
    "storage"
  ]
}
