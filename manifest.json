{
  "manifest_version": 2,
  "name": "Good Search",
  "description": "Good Search",
  "version": "0.0.0.1",
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html",
    "default_title": "Good Search"
  },
  "background": {
    "scripts": ["jquery.js", "background.js"]
  },
  "permissions": [
    "https://ajax.googleapis.com/",
    "webRequest",
    "webRequestBlocking",
    "https://*/*",
    "storage"
  ]
}
